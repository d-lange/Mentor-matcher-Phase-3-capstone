<h1>User Index</h1>

<% @users.each do |user| %>
	<div class="row">
	  <div class="small-2 large-4 columns"><%= image_tag user.picture_url %></div>
	  <div class="small-2 large-2 columns"><%= user.first_name %> <%= user.last_name %></div>
	  <div class="small-4 large-2 columns"><%= user.industry %></div>
	  <div class="small-4 large-4 columns"><%= user.mission_statement %></div>
	</div>
<hr>
<% end %>

